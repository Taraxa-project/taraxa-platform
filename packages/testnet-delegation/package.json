{
  "name": "@taraxa_project/taraxa-testnet-dpos",
  "description": "Taraxa Testnet Delegation contract",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "scripts": {
    "test": "yarn run hardhat test",
    "coverage": "yarn run hardhat coverage",
    "deploy-local": "TS_NODE_TRANSPILE_ONLY=1 npx hardhat run --network local ./scripts/deploy_plain.ts",
    "deploy-testnet": "env DEBUG='@openzeppelin:*' TS_NODE_TRANSPILE_ONLY=1 npx hardhat run --network testnet ./scripts/release.ts",
    "checkTx": "env DEBUG='@openzeppelin:*' TS_NODE_TRANSPILE_ONLY=1 npx hardhat run --network testnet ./scripts/receipt.ts",
    "checkTx:local": "env DEBUG='@openzeppelin:*' TS_NODE_TRANSPILE_ONLY=1 npx hardhat run --network local ./scripts/status.ts",
    "compile": "yarn run hardhat compile --network local"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.2.2",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-waffle": "^2.0.5",
    "@typechain/ethers-v5": "^10.2.0",
    "@typechain/hardhat": "^6.1.5",
    "@types/chai": "^4.3.4",
    "@types/ethereumjs-abi": "^0.6.3",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.15.5",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^5.2.0",
    "ethereum-waffle": "^3.4.4",
    "ethereumjs-abi": "^0.6.8",
    "ethereumjs-util": "^7.1.5",
    "ethers": "^5.7.2",
    "hardhat": "^2.9.9",
    "hardhat-gas-reporter": "^1.0.9",
    "solhint": "^3.4.1",
    "solidity-coverage": "^0.8.2",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@openzeppelin/contracts": "^4.6.0"
  }
}
